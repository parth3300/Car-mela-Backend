{
	"version": 2,
	"builds": [
	  {
		"src": "carmela/wsgi.py",
		"use": "@vercel/python",
		"config": {
		  "maxLambdaSize": "15mb",
		  "runtime": "python3.11"
		}
	  },
	  {
		"src": "build_files.sh",
		"use": "@vercel/static-build",
		"config": {
		  "distDir": "staticfilebuild"
		}
	  }
	],
	"routes": [
	  {
		"src": "/static/(.*)",
		"dest": "carmela/wsgi.py"
	  },
	  {
		"src": "/(.*)",
		"dest": "/static/$1"
	  }
	]
  }
  